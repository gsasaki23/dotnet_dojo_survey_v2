@model User
@{
    ViewData["Title"] = "Result";
}

<h2>Submitted Info</h2>
<div style="border:1px solid black; padding:5%;">
    <div style="display:inline-block;">
        <h3>Name:</h3>
        <h3>Dojo Location:</h3>
        <h3>Favorite Language:</h3>
        <h3>Comment</h3>
    </div>
    <div style="display:inline-block; vertical-align: top; margin-left: 20%;">
        <h3>@Model.FullName</h3>
        <h3>@Model.Location</h3>
        <h3>@Model.Language</h3>
        <h3>@Model.Comment</h3>
    </div>
    <a href="/" style="display:block;">Go Back</a>
</div>